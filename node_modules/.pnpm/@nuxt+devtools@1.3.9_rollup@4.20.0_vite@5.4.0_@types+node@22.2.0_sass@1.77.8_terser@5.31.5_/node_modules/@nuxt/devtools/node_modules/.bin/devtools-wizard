#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/srv/http/sentry-nuxt-bug/node_modules/.pnpm/@nuxt+devtools-wizard@1.3.9/node_modules/@nuxt/devtools-wizard/node_modules:/srv/http/sentry-nuxt-bug/node_modules/.pnpm/@nuxt+devtools-wizard@1.3.9/node_modules/@nuxt/node_modules:/srv/http/sentry-nuxt-bug/node_modules/.pnpm/@nuxt+devtools-wizard@1.3.9/node_modules:/srv/http/sentry-nuxt-bug/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/srv/http/sentry-nuxt-bug/node_modules/.pnpm/@nuxt+devtools-wizard@1.3.9/node_modules/@nuxt/devtools-wizard/node_modules:/srv/http/sentry-nuxt-bug/node_modules/.pnpm/@nuxt+devtools-wizard@1.3.9/node_modules/@nuxt/node_modules:/srv/http/sentry-nuxt-bug/node_modules/.pnpm/@nuxt+devtools-wizard@1.3.9/node_modules:/srv/http/sentry-nuxt-bug/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../@nuxt+devtools-wizard@1.3.9/node_modules/@nuxt/devtools-wizard/cli.mjs" "$@"
else
  exec node  "$basedir/../../../../../../@nuxt+devtools-wizard@1.3.9/node_modules/@nuxt/devtools-wizard/cli.mjs" "$@"
fi
